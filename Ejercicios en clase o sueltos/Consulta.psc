Algoritmo Consulta
	Definir Examenojo Como Real
	Definir examensangre Como Real
	Definir consultani Como Real
	examensangre <- 35000
	Examenojo <- 50000
	consultani <- 80000
	Definir sumaexa Como Real
	Definir total Como Real
	sumaexa <- (examensangre + Examenojo)
	total <- (consultani + sumaexa)
	Escribir "El valor total de los exámenes es:", sumaexa
	Escribir "El valor de la consulta es:", consultani
	Escribir "El valor total a pagar es:", total
	
FinAlgoritmo
